<Application x:Class="RigControlCreator.Windows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RigControlCreator.Windows"
			 Startup="AppStartup">
	<Application.Resources>

		<ResourceDictionary>

			<ResourceDictionary.MergedDictionaries>
				<!-- Set default skin -->
				<ResourceDictionary Source="/MayaTheme;component/Themes/Skins/QadskDarkStyle.xaml" />
				<ResourceDictionary Source="/MayaTheme;component/Themes/MayaStyle.xaml" />
			</ResourceDictionary.MergedDictionaries>

		</ResourceDictionary>

	</Application.Resources>
</Application>
